{
  "name": "textal",
  "version": "0.0.16",
  "description": "Tiny, dependency-free text utilities for JS/TS",
  "license": "MIT",
  "author": "aidankmcalister",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./clean": {
      "types": "./dist/clean/index.d.ts",
      "import": "./dist/clean/index.js",
      "require": "./dist/clean/index.js"
    },
    "./format": {
      "types": "./dist/format/index.d.ts",
      "import": "./dist/format/index.js",
      "require": "./dist/format/index.js"
    },
    "./search": {
      "types": "./dist/search/index.d.ts",
      "import": "./dist/search/index.js",
      "require": "./dist/search/index.js"
    },
    "./metrics": {
      "types": "./dist/metrics/index.d.ts",
      "import": "./dist/metrics/index.js",
      "require": "./dist/metrics/index.js"
    },
    "./style": {
      "types": "./dist/style/index.d.ts",
      "import": "./dist/style/index.js",
      "require": "./dist/style/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js"
    },
    "./dates": {
      "types": "./dist/dates/index.d.ts",
      "import": "./dist/dates/index.js",
      "require": "./dist/dates/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aidankmcalister/textal.git"
  },
"scripts": {
  "build": "rm -rf dist && tsc && tsc-alias",
  "prepublishOnly": "npm run build",
  "test": "vitest",
  "format": "prettier --write \"**/*.{ts,js,json,md}\"",
  "lint": "eslint . --ext .ts,.js",
  "lint:fix": "eslint . --ext .ts,.js --fix"
},
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.6.0",
    "vitest": "^3.2.4"
  }
}
